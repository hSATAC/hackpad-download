{
    "manifest_version": 2,
    "name": "Hackpad Downloader",
    "description": "Add download links to hackpad in different formats.",
    "version": "1.0.1",
    "content_scripts": [{
        "matches": [ "https://*.hackpad.com/*", "https://hackpad.com/*" ],
        "js": [ "zepto.min.js", "content.js" ],
        "css": [ "content.css" ],
        "run_at": "document_end",
        "all_frames": true
    }]
}
